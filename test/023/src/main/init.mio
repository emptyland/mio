package main

function makeMap = map 'weak value' {
    'name' <- 1.1F,
    'nick' <- 2.2D,
    'shit' <- 220
}

function main: void {
    var m = makeMap()
    val n = len(m)
    base::println('map size: '..n)
    base::fullGC()
    for (k, v in m) {
        base::println('k:'..k..', v:'..v)
    }
    base::fullGC()
    for (k, v in m) {
        base::println('k:'..k..', v:'..v)
    }
}